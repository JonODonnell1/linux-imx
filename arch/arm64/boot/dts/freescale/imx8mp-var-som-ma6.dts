			};

			i6_src4392: src4392@70 {
				compatible = "dummy";
				reg = <0x70>;
				status = "okay";
			};
		};

		i2c@6 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;

			o7_es9033: es9033@48 {
				compatible = "dummy";
				reg = <0x48>;
				status = "okay";
			};

			o7_es9033_sync: es9033@4c {  // before es9033 has clock initialized, _sync address must be used for write-only initialization of clock registers
				compatible = "dummy";
