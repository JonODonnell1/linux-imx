				status = "okay";
			};

			o3_es9033_sync: es9033@4c {  // before es9033 has clock initialized, _sync address must be used for write-only initialization of clock registers
				compatible = "dummy";
				reg = <0x4c>;
				status = "okay";
			};

			i3_es9820: es9820@20 {
				compatible = "dummy";
				reg = <0x20>;
