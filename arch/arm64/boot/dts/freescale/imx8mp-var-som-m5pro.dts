				status = "okay";
			};
		};

		i2c@3 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <3>;

			es9033@48 {
				compatible = "dummy";
				reg = <0x48>;
				status = "okay";
			};

			src4392@70 {
				compatible = "dummy";
				reg = <0x70>;
				status = "okay";
			};
		};

		i2c@4 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <4>;

			es9033@48 {
				compatible = "dummy";
				reg = <0x48>;
				status = "okay";
			};

			src4392@70 {
				compatible = "dummy";
				reg = <0x70>;
				status = "okay";
			};
		};

		i2c@5 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <5>;

			es9820@20 {
				compatible = "dummy";
				reg = <0x20>;
				status = "okay";
			};

			es9820@21 {
				compatible = "dummy";
				reg = <0x21>;
				status = "okay";
			};
		};

		i2c@6 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <6>;

			src4392@70 {
				compatible = "dummy";
				reg = <0x70>;
				status = "okay";
			};
		};

		i2c@7 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <7>;

			src4392@70 {
				compatible = "dummy";
				reg = <0x70>;
				status = "okay";
			};
		};
