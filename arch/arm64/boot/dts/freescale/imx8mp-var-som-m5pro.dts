 #include <dt-bindings/mux/mux.h>
 #include "imx8mp-var-som-ec_dsi_sai1.dtsi"

/ {
	model = "Variscite VAR-SOM-MX8M-PLUS-EC_DSI_SAI1 on M5Pro";

	chosen {
		stdout-path = &uart2;
	};

	reg_usdhc2_vmmc: regulator-usdhc2-vmmc {
		compatible = "regulator-fixed";
		regulator-name = "VSD_3V3";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		gpio = <&gpio4 22 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		startup-delay-us = <100>;
		off-on-delay-us = <12000>;
	};

	reg_usdhc2_vqmmc: regulator-usdhc2-vqmmc {
		compatible = "regulator-gpio";
		regulator-name = "VSD_VSEL";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <3300000>;
		gpio = <&gpio2 12 GPIO_ACTIVE_HIGH>;
		gpios-states = <1>;
		states = <3300000 0
			  1800000 1>;
	};

	sound_hdmi: sound_hdmi {
		compatible = "fsl,imx-audio-cdnhdmi";
		model = "audio-hdmi";
		audio-cpu = <&aud2htx>;
		hdmi-out;
		constraint-rate = <44100>,
				<88200>,
				<176400>,
				<32000>,
				<48000>,
				<96000>,
				<192000>;
		status = "okay";
	};

	dummy_dai1: dummy_dai1 {
		compatible = "linux,snd-soc-dummy";
		#sound-dai-cells = <0>;
//		status = "okay";
	};

	snd-sai1 {
		compatible = "simple-audio-card";
		status = "okay";
		simple-audio-card,name = "SAI1";
		simple-audio-card,format = "i2s";
		simple-audio-card,cpu {
			sound-dai = <&sai1>;
//			frame-master;
//			bitclock-master;
		};
		simple-audio-card,codec {
			sound-dai = <&dummy_dai1>;
		};
	};

	dummy_dai2: dummy-dai2 {
		compatible = "linux,snd-soc-dummy";
		#sound-dai-cells = <0>;
		status = "okay";
	};

	sound-sai2 {
		compatible = "simple-audio-card";
		status = "okay";
		simple-audio-card,name = "SAI2";
		simple-audio-card,format = "i2s";
		simple-audio-card,cpu {
			sound-dai = <&sai2>;
//			frame-master;
//			bitclock-master;
		};
		simple-audio-card,codec {
			#sound-dai-cells = <0>;
			sound-dai = <&dummy_dai2>;
		};
	};

	extcon_usb1: extcon_usb1 {
		compatible = "linux,extcon-usb-gpio";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_extcon>;
		id-gpio = <&gpio1 5 GPIO_ACTIVE_HIGH>;
		status = "okay";
	};
};

&aud2htx {
	status = "okay";
};

&eqos {
	mdio {
		ethphy1: ethernet-phy@5 {
			compatible = "ethernet-phy-ieee802.3-c22";
			reg = <5>;
			at803x,eee-disabled;
			at803x,vddio-1p8v;
			eee-broken-1000t;
		};
	};
};

&i2c4 {
	clock-frequency = <400000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c4>;
	scl-gpios = <&gpio5 20 (GPIO_ACTIVE_HIGH|GPIO_OPEN_DRAIN)>;
	sda-gpios = <&gpio5 21 (GPIO_ACTIVE_HIGH|GPIO_OPEN_DRAIN)>;
	status = "okay";

	/* mcp7940x RTC module */
	rtc@6f {
		compatible = "microchip,mcp7940x";
		reg = <0x6f>;
		status = "okay";
	};

//	/* Capacitive touch controller */
	ctp: ctp@38 {
		compatible = "edt,edt-ft5206";
		reg = <0x38>;
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_ctp>;
		interrupt-parent = <&gpio5>;
		interrupts = <3 IRQ_TYPE_EDGE_FALLING>;
		wake-gpios = <&gpio5 5 GPIO_ACTIVE_HIGH>;
		touchscreen-size-x = <800>;
		touchscreen-size-y = <480>;
		touchscreen-inverted-x;
		touchscreen-inverted-y;
		wakeup-source;
		status = "okay";
	};

	icn6211@2c {
		compatible = "dummy";
		reg = <0x2c>;
		status = "okay";
	};

	pi6cfgl601b@69 {
		compatible = "dummy";
		reg = <0x69>;
		status = "okay";
	};
};

&i2c2 {
	clock-frequency = <400000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c2>;
