				status = "okay";
			};

			src4392@70 {
				compatible = "dummy";
				reg = <0x70>;
				status = "okay";
			};
		};

		i2c@1 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <1>;

			es9033@48 {
				compatible = "dummy";
